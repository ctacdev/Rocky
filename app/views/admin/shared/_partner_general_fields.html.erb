<fieldset>
  <legend>General</legend>
  <ul class="stacked">
    <li class="width_b">
      <h3><%= f.label :name %></h3>
      <%= field_div f, :name, class: 'form-control' %>
      <span class="error"><%= @partner.errors[:name].join("\n") %></span>
    </li>
    <li class="width_a">
      <h3><%= f.label :organization %></h3>
      <%= field_div f, :organization, class: 'form-control' %>
      <span class="error"><%= @partner.errors[:organization].join("\n") %></span>
    </li>
    <li class="width_a">
      <h3><%= f.label :url %></h3>
      <%= field_div f, :url, class: 'form-control' %>
      <span class="error"><%= f.object.errors[:url].join("\n") %></span>
    </li>
    
    <li class="width_a">
      <h3><%= f.label :email %></h3>
      <%= field_div f, :email, class: 'form-control' %>
      <span class="error"><%= @partner.errors[:email].join("\n") %></span>
    </li>     
    <li class="width_a">
      <h3><%= f.label :phone %></h3>
      <%= field_div f, :phone, class: 'form-control' %>
      <span class="error"><%= f.object.errors[:phone].join("\n") %></span>
    </li>
     
  </ul>
  
  <ul class="stacked">
    <li class="width_a">
      <h3><%= f.label :address %></h3>
      <%= field_div f, :address, class: 'form-control' %>
      <span class="error"><%= f.object.errors[:address].join("\n") %></span>
    </li>
  </ul>
  <ul class="stacked">
    <li class="width_a">
      <h3><%= f.label :city %></h3>
      <%= field_div f, :city, class: 'form-control' %>
      <span class="error"><%= f.object.errors[:city].join("\n") %></span>
    </li>
    <li class="width_b">
      <h3><%= f.label :state_abbrev, "State" %></h3>
      <div><%= f.select :state_abbrev, GeoState.collection_for_select, :include_blank => "select a state", class: 'form-control' %></div>
      <span class="error"><%= f.object.errors[:state_abbrev].join("\n") %></span>
    </li>
    <li class="width_c">
      <h3><%= f.label :zip_code %></h3>
      <%= field_div f, :zip_code, class: 'form-control' %>
      <span class="error"><%= f.object.errors[:zip_code].join("\n") %></span>
    </li>
  </ul>
  
  
</fieldset>
